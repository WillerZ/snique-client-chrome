{
  "name": "Snique",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Snique message reader.",
  "content_scripts":
    [
      {
	"matches": ["http://*/*","https://*/*"],
	"js": ["findresources.js"],
	"run_at": "document_idle"
      }
    ],
  "permissions": ["storage","notifications","http://*/","https://*/"],
  "web_accessible_resources": ["snique.png"],
  "background":
  {
    "scripts": ["aes.js","background.js"]
  },
  "options_page": "settings.html"
}
